<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- parameterType 내용을 줄이기 위해서 사용
        todolist_project.login.UserDTO 를 user 로 치환 한다고 생각하면 편함
    -->
    <typeAliases>
        <typeAlias type="todolist_project.login.UserDTO" alias="user"></typeAlias>
        <typeAlias type="todolist_project.main.TodoListDTO" alias="todolist"></typeAlias>
        <typeAlias type="todolist_project.main.UploadDTO" alias="upload"></typeAlias>
    </typeAliases>

</configuration>